{
  "schemaVersion": "1.3",
  "acapPackageConf": {
    "setup": {
      "friendlyName": "Docker Daemon",
      "appId": "414120",
      "appName": "dockerdwrapper",
      "vendor": "Axis Communications",
      "embeddedSdkVersion": "3.0",
      "user": {
        "group": "root",
        "username": "root"
      },
      "vendorUrl": "https://www.axis.com",
      "runMode": "once",
      "version": "1.5.0-LTS"
    },
    "installation": {
      "postInstallScript": "postinstallscript.sh"
    },
    "configuration": {
      "paramConfig": [
        {
          "name": "SDCardSupport",
          "default": "no",
          "type": "enum:no|No, yes|Yes"
        },
        {
          "name": "UseTLS",
          "default": "yes",
          "type": "enum:no|No, yes|Yes"
        },
        {
          "name": "TCPSocket",
          "default": "yes",
          "type": "enum:no|No, yes|Yes"
        },
        {
          "name": "IPCSocket",
          "default": "no",
          "type": "enum:no|No, yes|Yes"
        }
      ]
    }
  }
}
