{
    "schemaVersion": "1.3",
    "acapPackageConf": {
        "setup": {
            "friendlyName": "Docker Daemon with Compose",
            "appId": "414150",
            "appName": "dockerdwrapperwithcompose",
            "vendor": "Axis Communications",
            "embeddedSdkVersion": "3.0",
            "user": {
                "group": "root",
                "username": "root"
            },
            "vendorUrl": "https://www.axis.com",
            "runMode": "once",
            "version": "1.3.0"
        },
        "installation": {
            "postInstallScript": "postinstallscript.sh"
        },
        "uninstallation": {
            "preUninstallScript": "preuninstallscript.sh"
        },
        "configuration": {
            "paramConfig": [
                {
                    "name": "SDCardSupport",
                    "default": "no",
                    "type": "enum:no|No, yes|Yes"
                },
                {
                    "name": "UseTLS",
                    "default": "yes",
                    "type": "enum:no|No, yes|Yes"
                },
                {
                    "name": "IPCSocket",
                    "default": "yes",
                    "type": "enum:no|No, yes|Yes"
                }
            ]
        }
    }
}
